        <agent x="217" y="235" color="white" font-size="24" text="VALUS"/>
        <agent 
            img="sword.png" x="220" y="275" width="64" height="64" draggable="true" 
            on-drop="unlock" on-zone="unlocked" on-trigger="unlocked"
        >
            <unlocked x="200" />
        </agent>    
        
        <agent img="map.jpeg" x="50" y="350" width="64" height="64" on-touch="_GOTO_PAGE:2" />
        
